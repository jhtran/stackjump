# Generated by Chef.
# Local modifications will be overwritten.

auto lo
iface lo inet loopback

auto eth0
iface eth0 inet manual
  bond-master bond0

auto eth1
iface eth1 inet manual
  bond-master bond0
  # Force eth0 to come up first.
  # Allows bond0 will use eth0's mac address.
  pre-up sleep 10

auto eth2
iface eth2 inet manual
  bond-master bond1

auto eth3
iface eth3 inet manual
  bond-master bond1
  # Force eth2 to come up first.
  # Allows bond1 will use eth2's mac address.
  pre-up sleep 10

auto bond1
iface bond1 inet manual
  bond-mode 802.3ad
  bond-slaves none
  bond-miimon 100
  bond-downdelay 200
  bond-updelay 200
  bond-xmit-hash-policy 1
  bond-ad-select 1
  bond-lacp-rate 1

source /etc/network/interfaces.d/*
