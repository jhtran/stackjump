{
  "name": "myzone1",
  "default_attributes": {
    "database-backup": {
      "staas": {
        "directory": "myzone1"
      },
      "cron": {
        "hour": "4,12,20",
        "minute": "38"
      }
    },
    "dns": {
      "public_forward_zone": "myzone1.attcompute.com",
      "public_forward_domain": "myzone1.attcompute.com"
    },
    "graphite": {
      "server_hostname": "monitoring.myzone1.attcompute.com",
      "cas_root_proxy_url": "https://monitoring.myzone1.attcompute.com:8443"
    },
    "infra-monitoring": {
      "ipmi": {
      },
      "ssl_checks": {
        "additional_hosts": [
          "monitoring.myzone1.attcompute.com:443",
          "dashboard.myzone1.attcompute.com:443"
        ]
      }
    },
    "nagios": {
      "server_hostname": "monitoring.myzone1.attcompute.com",
      "cas_root_proxy_url": "https://monitoring.myzone1.attcompute.com"
    },
    "IPS": {
      "network": "192.168.1.0",
      "netmask": "255.255.255.0",
      "bond": "bond0",
      "primary_gw": "192.168.112.129",
      "secondary_gw": "192.168.112.193"
    },
    "vizgems": {
    },
    "vms": {
      "sysconfig": {
        "shared_volume": "/vol/gridcentric",
        "vms_user": "libvirt-qemu",
        "vms_group": "kvm"
      }
    }
  },
  "json_class": "Chef::Role",
  "env_run_lists": {
  },
  "run_list": [
    "recipe[openstack-base::myzone1]",
    "role[base]"
  ],
  "description": "A role that all MYZONE1 nodes will have.",
  "chef_type": "role",
  "override_attributes": {
  }
}
