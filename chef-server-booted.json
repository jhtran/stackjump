{
  "run_list": [
    "recipe[chef-client]",
    "recipe[infra-management::chef-repo]",
    "recipe[networking]"
  ],
  "reboot-handler": {
    "post_boot_runlist": [
      "recipe[infra-management::chef-repo]",
      "role[chef-server]"
    ]
  },
  "zone": "myzone1",
  "infra-management": {
    "ghuser": "my-ro-user",
    "ghpw": "my-ro-pw",
    "decryptpw": "my-decrypt-pw"
  },
  "networking": {
    "interfaces": {
      "bond0": {
        "address": "192.168.112.11",
        "netmask": "255.255.255.128",
        "bond-mode": "active-backup",
        "dns-nameservers": [ "8.8.8.8" ],
        "gateway": "192.168.112.1"
      },
      "bond1.2001": {
        "address": "192.168.128.11",
        "netmask": "255.255.255.128"
      },
      "bond1.2002": {
        "address": "75.55.105.11",
        "netmask": "255.255.240.0",
        "gateway": "75.55.105.1"
      }
    },
    "udev": {
      "bus_order": [
        ["0000:00:05.0","0000:00:06.0","0000:00:07.0","0000:00:08.0"]
      ]
    }
  }
}
